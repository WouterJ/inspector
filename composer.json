{
    "name": "wouterj/inspector",
    "description": "A fast inspector to inspect and edit files",
    "authors": [
        {
            "name": "Wouter J",
            "email": "wouter@wouterj.nl"
        }
    ],

    "require": {
        "symfony/finder": "2.1.x",
        "zendframework/zend-eventmanager": "2.0.x",
        "symfony/console": "2.1.x",
        "pimple/pimple": "1.x"
    },
    "require-dev": {
        "symfony/filesystem": "2.1.x",
        "behat/behat": "2.4.x"
    },

    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
